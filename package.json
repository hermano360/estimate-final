    "jszip": "^2.6.1",
    "lodash": "^4.17.5",
    "moment": "^2.20.1",
    "mongodb": "^2.2.33",
    "nodemailer": "^4.0.1",
    "querystring": "^0.2.0",
    "react": "^15.4.0",
    "react-apollo": "^2.1.3",
