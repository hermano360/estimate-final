    "clean": "rimraf ./dist/*.js && rimraf ./dist/*.map",
    "build": "NODE_ENV=production npm run clean && webpack -p",
    "serve": "webpack-dev-server",
    "test": "node scripts/test.js --env=jsdom"
  },
  "keywords": [],
